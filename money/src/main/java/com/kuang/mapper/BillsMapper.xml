<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kuang.mapper.BillsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.kuang.entity.Bills">
        <id column="id" property="id" />
        <result column="title" property="title" />
        <result column="billtime" property="billtime" />
        <result column="typeid" property="typeid" />
        <result column="price" property="price" />
        <result column="explains" property="explains" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, title, billtime, typeid, price, explains
    </sql>

</mapper>
